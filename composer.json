{
  "name": "remotelyliving/doorkeeper",
  "description": "A Feature Toggle",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": ["Doorkeeper", "Feature Flag"],
  "authors": [
    {
      "name": "Christian Thomas",
      "email": "christian.h.thomas@me.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "psr/http-message": "^1.0",
    "ramsey/uuid": "^3.8 || ^4.0",
    "ext-json": "*",
    "ext-filter": "*",
    "ext-mbstring": "*",
    "phpstan/phpstan": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "maglnet/composer-require-checker": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "RemotelyLiving\\Doorkeeper\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RemotelyLiving\\Doorkeeper\\Tests\\" : "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ocramius/package-versions": true
    }
  }
}
