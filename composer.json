{
  "name": "remotelyliving/doorkeeper",
  "description": "A Feature Toggle",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": ["Doorkeeper", "Feature Flag"],
  "authors": [
    {
      "name": "Christian Thomas",
      "email": "christian.h.thomas@me.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "psr/http-message": "^1.0",
    "ext-json": "*",
    "ext-filter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "squizlabs/php_codesniffer": "^3.0",
    "php-coveralls/php-coveralls": "^1.0",
    "phpstan/phpstan": "^0.10.2",
    "maglnet/composer-require-checker": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "RemotelyLiving\\Doorkeeper\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RemotelyLiving\\Doorkeeper\\Tests\\" : "tests/"
    }
  }
}
